# .env - Environment variables for AI-Algorithms repo

# OpenAI API (for NewsSentimentAgent, LLM calls)
OPENAI_API_KEY=your_openai_api_key_here

# TwelveData (OHLC data)
TWELVEDATA_API_KEY=your_twelvedata_api_key_here

# Alpha Vantage (alternative OHLC data)
ALPHAVANTAGE_API_KEY=your_alpha_vantage_api_key_here

# NewsAPI (fetch_news.py)
NEWSAPI_KEY=your_newsapi_key_here

# Reddit API (social_sentiment notebook)
REDDIT_ID=your_reddit_client_id_here
REDDIT_SECRET=your_reddit_client_secret_here
REDDIT_USER_AGENT=your_app_user_agent_here

# Telegram Bot (signal alerts)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_CHAT_ID=your_telegram_chat_id_here

# Binance API (optional: for private endpoints/order books)
BINANCE_API_KEY=your_binance_api_key_here
BINANCE_API_SECRET=your_binance_api_secret_here
